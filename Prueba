import pandas as pd
import numpy as np
import matplotlib.pyplot as plt

def Grafplot (nombre,Estaturas):
    plt.bar(nombre,Estaturas,color="#FC1389")
    plt.show()


nombre=("Messi","Neymar","El bicho","Pique","Ochoa","Fabi√±o","Quequo","Tecatito")
Estaturas=[1.69,1.75,1.87,1.91,1.92,1.88,1.58,1.74]
Grafplot(nombre,Estaturas)
